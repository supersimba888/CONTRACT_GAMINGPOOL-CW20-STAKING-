    {
      "description": "Returns the current status of Vesting for specified category wallet address",
      "type": "object",
      "required": [
        "vesting_details"
      ],
      "properties": {
        "vesting_details": {
          "type": "object",
          "required": [
            "address"
          ],
          "properties": {
            "address": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    }
  ]
}
